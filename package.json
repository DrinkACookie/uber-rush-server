{
  "name": "clone-coding-uber",
  "version": "1.0.0",
  "description": "cloning uber",
  "main": "index.js",
  "scripts": {
    "predev": "yarn run types",
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cd src && copy ./api/**/*.graphql ../dist/api",
    "start": "cd dist && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrinkACookie/Clone-coding-Uber.git"
  },
  "author": "park geon woo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DrinkACookie/Clone-coding-Uber/issues"
  },
  "homepage": "https://github.com/DrinkACookie/Clone-coding-Uber#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.46",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mailgun-js": "^0.22.7",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.5",
    "@types/twilio": "^2.11.0",
    "babel-runtime": "^6.26.0",
    "copy": "^0.3.2",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^9.0.0",
    "mailgun-js": "^0.22.0",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "pg": "^8.0.3",
    "twilio": "^3.44.0",
    "typeorm": "^0.2.24"
  }
}
